    <string name="cmd_sres">Show screen resolution info\n</string>
    <string name="cmd_netinfo">Show network info\n</string>
    <string name="cmd_fontsize">Show/Set font size\n
                                > fontsize\n
                                > fontsize [BIG | MEDIUM | SMALL]\n</string>
    <string name="usage_fontsize">Usage: \n
                             > fontsize\n
                             > fontsize [BIG | MEDIUM | SMALL]\n</string>
    <string name="cmd_exit">Exit console\n</string>
    
