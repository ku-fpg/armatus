    <string name="cmd_mkdir">Make directory\n
                             > mkdir /path/to/dir\n</string>
    <string name="usage_mkdir">Usage: mkdir /path/to/dir</string>
    <string name="cmd_ren">Rename file or directory\n
                           > ren path/to/src path/to/dest\n</string>
    <string name="usage_ren">Usage: ren /path/to/src /path/to/dest</string>
    <string name="cmd_cp">Copy file\n
                          > cp /path/to/srcfile /path/to/destfile\n</string>
    <string name="usage_cp">Usage: cp /path/to/srcfile /path/to/destfile</string>
    <string name="cmd_cleardex">Clear cached dex files\n</string>
