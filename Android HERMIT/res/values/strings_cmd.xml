<resources>
    <string name="cmd_help">Type \'help [command]\' to show detail about the command\n</string>
    <string name="cmd_unknown">Unknown command</string>
    <string name="cmd_ls">List files and directory\n
                          > ls\n
                          > ls /path/to/dir\n\n
                          To turn on/off detail listing of files and directories\n
                          > ls -f\n</string>
    <string name="usage_ls">Usage: ls [dest dir]</string>
    <string name="cmd_pwd">Print current path\n</string>
    <string name="cmd_cd">Change current directory to target directory\n
                          > cd /path/to/dir\n</string>
    <string name="usage_cd">Usage: cd [directory]</string>
    <string name="cmd_clear">Clear screen\n</string>
    <string name="cmd_run">Execute a apk\n
                           > run /path/to/apk [args]\n</string>
    <string name="usage_run">Usage: run /path/to/apk [args]</string>
    <string name="cmd_history">Show command history\n
                               Use \'!\' and command index to repeat the command\n
                               e.g.\n
                               > !100\n</string>
